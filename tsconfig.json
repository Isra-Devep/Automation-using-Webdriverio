{
    "compilerOptions": {
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "module": "commonjs",
        "types": [
            "node",
            "@wdio/globals/types",
            "expect-webdriverio",
            "@wdio/cucumber-framework"
        ],
        "baseUrl": "main",
        "paths":{
            "wdioUi/*":["ui/*"],
            "wdioUtils/*":["utils/*"],
            "wdioTestData/*":["testData/*"]
        },
        "target": "es2022"
    }
}