{
    "compilerOptions": {
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "module": "commonjs",
        "types": [
            "node",
            "@wdio/globals/types",
            "expect-webdriverio",
            "@wdio/cucumber-framework"
        ],
        "paths":{
            "wdioUi/*":["./main/ui/*"],
            "wdioUtils/*":["./main/utils/*"],
            "wdioTestData/*":["./main/testData/*"]
        },
        "target": "es2022"
    }
}